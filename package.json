{
  "name": "bootstrap-web",
  "version": "1.0.0",
  "description": "curso bootstrap-web",
  "main": "index.js",
  "scripts": {
    "dev": "lite-server",
    "start": "concurrently \"npm run watch:scss \" \"npm run dev \"",
    "test": "echo \"Error: no test specified\" && exit 1",
    "scss": "node-sass -o css/ css/",
    "watch:scss": "onchange 'css/*.scss' -- npm run scss ",
    "clean": "rimraf dist",
    "imagemin": "imagemin images/* --out-dir dist/images",
    "usemin": "usemin index.html -d dist --htmlmin -o dist/index.html && usemin about.html -d dist --htmlmin -o dist/about.html && usemin precio.html -d dist --htmlmin -o dist/precio.html && usemin contact.html -d dist --htmlmin -o dist/contact.html",
    "build": "npm run clean && npm run scss && npm run imagemin && npm run usemin"
  },
  "author": "jisus",
  "license": "ISC",
  "dependencies": {
    "bootstrap": "^5.3.3",
    "concurrently": "^8.2.2",
    "jquery": "^3.7.1",
    "open-iconic": "^1.1.1",
    "popper.js": "^1.16.1"
  },
  "devDependencies": {
    "copyfiles": "^2.4.1",
    "cssmin": "^0.4.3",
    "htmlmin": "^0.0.7",
    "lite-server": "^2.6.1",
    "node-sass": "^9.0.0",
    "onchange": "^7.1.0",
    "rimraf": "^6.0.1",
    "uglifyjs": "^2.4.11",
    "usemin-cli": "^0.6.0"
  }
}
